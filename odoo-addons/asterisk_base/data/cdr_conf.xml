<?xml version="1.0" encoding="utf-8"?>

<odoo>
  <data>
    <record id="asterisk_cdr_conf" model="asterisk.conf">
      <field name="filename">cdr.conf</field>
      <field name="filename_on_create">cdr.conf</field>
      <field name="server" ref="asterisk_test_server"/>
      <field name="server_on_create" ref="asterisk_test_server"/>
      <field name="content">[general]
enable = yes
unanswered = yes
congestion = yes
endbeforehexten = yes

[csv]
usegmtime=no
newcdrcolumns=yes
loguniqueid=yes
loguserfield=yes
      </field>
    </record>
  </data>
</odoo>
