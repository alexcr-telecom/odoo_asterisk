<?xml version="1.0" encoding="utf-8"?>

<odoo>
  <data>
    <record id="asterisk_cdr_adaptive_odbc_conf" model="asterisk.conf">
      <field name="filename">cdr_adaptive_odbc.conf</field>
      <field name="server" ref="asterisk_test_server"/>
      <field name="filename_on_create">cdr_adaptive_odbc.conf</field>
      <field name="server_on_create" ref="asterisk_test_server"/>
      <field name="content">[cdr_odoo_odbc]
connection=odoo_odbc
table=asterisk_cdr
usegmtime=no
alias start => started
alias end => ended
alias answer => answered
      </field>
    </record>
  </data>
</odoo>
